<div class="page-header">
  <h3><%= @user.username %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">

    <% @user.photos.each do |photo| %>
      <tr>
        <td><%= photo.caption %></td>
        <td><a href="/photos/<%= photo.id %>">
          <img src="<%= photo.image %>" alt="" class="img-responsive"/></td>

      </tr>
      <%end%>
  </table>




    </div>
  </div>
