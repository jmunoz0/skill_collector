<div class="page-header">
  <h1>
    All Users

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">


      <% @users.each do |user| %>
      <tr>
        <%if user.id==current_user.id%>
        <td><a href="/positions">  <%= user.username%></a>
        </td>
        <%else%>
        <td><a href="/users/<%= user.id %>">  <%= user.username%></a>
        </td>
        <%end%>

      </tr>
      <% end %>
    </table>
  </div>
</div>
